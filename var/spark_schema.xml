<?xml version="1.0" encoding="UTF-8"?>
<schema metamodelVersion="4.0" name="Spark">
   <Cube name="Taxi">
      <Annotations xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:string" />
      <Dimensions>
         <Dimension key="ID" name="Customer" table="store" visible="true">
            <Attributes>
               <Attribute hasHierarchy="false" levelType="Regular" name="ID">
                  <Key>
                     <Column name="t_id" />
                  </Key>
               </Attribute>
               <Attribute hasHierarchy="true" levelType="Regular" name="Medallion">
                  <Key>
                     <Column name="medallion" />
                  </Key>
               </Attribute>
               <Attribute hasHierarchy="true" levelType="Regular" name="Hack">
                  <Key>
                     <Column name="hack_license" />
                  </Key>
               </Attribute>
               <Attribute hasHierarchy="true" levelType="Regular" name="Rate Code">
                  <Key>
                     <Column name="rate_code" />
                  </Key>
               </Attribute>
               <Attribute hasHierarchy="true" levelType="Regular" name="Pickup Longitude">
                  <Key>
                     <Column name="pickup_longitude" />
                  </Key>
               </Attribute>
               <Attribute hasHierarchy="true" levelType="Regular" name="Pickup Latitude">
                  <Key>
                     <Column name="pickup_latiude" />
                  </Key>
               </Attribute>
            </Attributes>
            <Hierarchies />
         </Dimension>
         <Dimension key="ID" name="Time" table="store" visible="true">
         	 <Attributes>
               <Attribute hasHierarchy="false" levelType="Regular" name="ID">
                  <Key>
                     <Column name="t_id" />
                  </Key>
               </Attribute>
               <Attribute hasHierarchy="true" levelType="Regular" name="Day">
               		<Key>
               			<Column name="day"/>
               		</Key>
               	</Attribute>
               <Attribute hasHierarchy="true" levelType="Regular" name="Hour">
                  <Key>
                     <Column name="hour" />
                  </Key>
               </Attribute>
           </Attributes>
           <Hierarchies />
         </Dimension>
      </Dimensions>
      <MeasureGroups>
         <MeasureGroup name="Default" table="store">
            <DimensionLinks>
               <FactLink dimension="Customer" />
               <FactLink dimension="Time"/>
            </DimensionLinks>
            <Measures>
               <Measure aggregator="sum" column="passenger_count" name="Passenger Count" visible="true" />
               <Measure aggregator="avg" column="passenger_count" name="Avg Passenger Count" visible="true" />
               <Measure aggregator="sum" column="trip_distance" name="Trip Distance" visible="true" />
               <Measure aggregator="avg" column="trip_distance" name="Avg Trip Distance" visible="true" />
               <Measure aggregator="max" column="trip_distance" name="Max Trip Distance" visible="true" />
               <Measure aggregator="sum" column="trip_time_in_secs" name="Trip Time(secs)" visible="true" />
               <Measure aggregator="avg" column="trip_time_in_secs" name="Avg Trip Time(secs)" visible="true" />
            </Measures>
         </MeasureGroup>
      </MeasureGroups>
   </Cube>
   <PhysicalSchema>
      <Table name="store">
         <Key>
            <Column name="t_id" />
         </Key>
      </Table>
   </PhysicalSchema>
</schema>